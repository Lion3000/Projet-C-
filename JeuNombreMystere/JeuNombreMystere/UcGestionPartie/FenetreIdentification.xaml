<Window x:Class="JeuNombreMystere.UcGestionPartie.FenetreIdentification"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JeuNombreMystere.UcGestionPartie"
        mc:Ignorable="d"
        Title="Partie" Height="300" Width="300">
    <StackPanel HorizontalAlignment="Center" Margin="30,0,24.6,0.4" Width="239" >
        <Label HorizontalAlignment="Center" Content="IDENTIFICATION" VerticalAlignment="Top"  DockPanel.Dock="Left"/>
        <DockPanel LastChildFill="False" Margin="0,20,0,10">
            <Label Content="Nom" />
            <TextBox Name="Nom" Width="200" />
        </DockPanel>
        <DockPanel LastChildFill="False" Margin="0,10,0,50">
            <Label Content="Login" />
            <TextBox Name="Login" Width="200" />
        </DockPanel>
        <Button HorizontalAlignment="Center" Name="Jouer" Content="Jouer" Width="60" Click="Jouer_Click"/>
    </StackPanel>

       
</Window>
